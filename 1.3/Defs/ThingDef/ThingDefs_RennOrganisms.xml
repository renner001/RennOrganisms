<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- RennPond -->
  <ThingDef ParentName="BuildingBase" Class="DanielRenner.RennOrganisms.Building_RennBaseDef">
    <defName>RennPond</defName>
    <label>renn pond</label>
    <description>A pond inhabited by renn organisms. Renn microbes are invisibly small and have a peculiar connection to reality: \nAnchient stories speak of friends to the microbes. Their friends are always favored by luck and rarely see enemies at their gates. Yet their societies are plagued by infighting and mood swings. \nOne can't have everything, it seems.\n\nRenn ponds offer a flat reduction of threats by 250 threat points or 12% - whatever is bigger.\nThey impose a -3 penalty to the mood of your people.</description>
    <thingClass>DanielRenner.RennOrganisms.Building_RennPond</thingClass>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Building_RennPond/RennPond</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(6,6)</drawSize>
      <!--<shadowData>
        <volume>(6.03,1.0,5.25)</volume>
        <offset>(0,0,-0.25)</offset>
      </shadowData>-->
      <damageData>
        <rect>(0.3,0,5.4,6)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>false</blockWind>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <fillPercent>1</fillPercent>
    <blockLight>false</blockLight>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>5000</MaxHitPoints>
      <WorkToBuild>12000</WorkToBuild>
      <Flammability>0.0</Flammability>
      <Beauty>10</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(6,6)</size>
    <costList>
      <WoodLog>100</WoodLog>
    </costList>
    <baseMoodEffectOfBuildingUnpoweredAndUnfueled>3</baseMoodEffectOfBuildingUnpoweredAndUnfueled>
    <moodOffsetOfBuildingPowered>0</moodOffsetOfBuildingPowered>
    <moodToThreatMultiplier>0.04</moodToThreatMultiplier>
    <maxThreatCap>500000</maxThreatCap>
    <minThreatReduction>250</minThreatReduction>
    <moodOffsetFedRennFiber>3</moodOffsetFedRennFiber>
    <moodOffsetFedRennFiberDomestic>6</moodOffsetFedRennFiberDomestic>
    <moodOffsetFedRennPowder>9</moodOffsetFedRennPowder>
    <moodOffsetFedRennConcentrate>12</moodOffsetFedRennConcentrate>
    <moodOffsetFedRennCapsule>15</moodOffsetFedRennCapsule>
    <availableFeedOptions>
      <li>RennFiber</li>
      <li>RennFiberDomestic</li>
    </availableFeedOptions>
    <comps>
      <li Class="DanielRenner.RennOrganisms.CompProperties_SpecificRefuelable">
        <fuelConsumptionRate>10.0</fuelConsumptionRate>
        <fuelCapacity>30.0</fuelCapacity>
        <fuelFilter>
          <thingDefs>
            <li>RennFiber</li>
            <li>RennFiberDomestic</li>
          </thingDefs>
        </fuelFilter>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>10</glowRadius>
        <glowColor>(80,112,180,0)</glowColor>
      </li>
    </comps>
    <rotatable>false</rotatable>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
    <!--<placeWorkers>
      <li>PlaceWorker_OnSteamGeyser</li>
    </placeWorkers>-->
    <building>
      <!--<ai_chillDestination>false</ai_chillDestination>
      <soundAmbient>GeothermalPlant_Ambience</soundAmbient>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>-->
    </building>
    <constructEffect>ConstructMetal</constructEffect>
    <!--<designationHotKey>Misc6</designationHotKey>-->
    <researchPrerequisites>
      <li>Research_RennPonds</li>
    </researchPrerequisites>
    <!--<constructionSkillPrerequisite>0</constructionSkillPrerequisite>-->
  </ThingDef>
  <!-- RennReactor-->
  <ThingDef ParentName="BuildingBase"  Class="DanielRenner.RennOrganisms.Building_RennBaseDef">
    <defName>RennReactor</defName>
    <label>Renn tank</label>
    <description>A tank inhabited by a dense amount of renn microbes. Renn microbes are invisibly small and have a peculiar connection to reality: \nAnchient stories speak of friends to the microbes. Their friends are always favored by luck and rarely see enemies at their gates. Yet their societies are plagued by infighting and mood swings. \nOne can't have everything, it seems.\n\nRenn tanks offer a flat reduction of threats by 5000 threat points or 24% - whatever is bigger.\nThey impose a -6 penalty to the mood of your people.</description>
    <thingClass>DanielRenner.RennOrganisms.Building_RennReactor</thingClass>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Building_RennReactor/RennReactor</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(6,6)</drawSize>
      <!--<shadowData>
        <volume>(6.03,1.0,5.25)</volume>
        <offset>(0,0,-0.25)</offset>
      </shadowData>-->
      <damageData>
        <rect>(0.3,0,5.4,6)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>true</blockWind>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <fillPercent>1</fillPercent>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>5000</MaxHitPoints>
      <WorkToBuild>12000</WorkToBuild>
      <Flammability>0.0</Flammability>
      <Beauty>10</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(6,6)</size>
    <costList>
      <Steel>300</Steel>
      <ComponentIndustrial>4</ComponentIndustrial>
    </costList>
    <baseMoodEffectOfBuildingUnpoweredAndUnfueled>6</baseMoodEffectOfBuildingUnpoweredAndUnfueled>
    <moodOffsetOfBuildingPowered>0</moodOffsetOfBuildingPowered>
    <moodToThreatMultiplier>0.04</moodToThreatMultiplier>
    <maxThreatCap>500000</maxThreatCap>
    <minThreatReduction>1000</minThreatReduction>
    <moodOffsetFedRennFiber>3</moodOffsetFedRennFiber>
    <moodOffsetFedRennFiberDomestic>6</moodOffsetFedRennFiberDomestic>
    <moodOffsetFedRennPowder>9</moodOffsetFedRennPowder>
    <moodOffsetFedRennConcentrate>12</moodOffsetFedRennConcentrate>
    <moodOffsetFedRennCapsule>15</moodOffsetFedRennCapsule>
    <availableFeedOptions>
      <li>RennFiberDomestic</li>
      <li>RennPowder</li>
      <li>RennConcentrate</li>
    </availableFeedOptions>
    <comps>
      <li Class="CompProperties_Flickable"/>
      <li Class="DanielRenner.RennOrganisms.CompProperties_SpecificRefuelable">
        <fuelConsumptionRate>10.0</fuelConsumptionRate>
        <fuelCapacity>30.0</fuelCapacity>
        <consumeFuelOnlyWhenPowered>true</consumeFuelOnlyWhenPowered>
        <fuelFilter>
          <thingDefs>
            <li>RennFiberDomestic</li>
            <li>RennPowder</li>
            <li>RennConcentrate</li>
          </thingDefs>
        </fuelFilter>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>4</glowRadius>
        <glowColor>(80,112,180,0)</glowColor>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>false</shortCircuitInRain>
        <basePowerConsumption>2000</basePowerConsumption>
      </li>
    </comps>
    <rotatable>false</rotatable>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
    <!--<placeWorkers>
      <li>PlaceWorker_OnSteamGeyser</li>
    </placeWorkers>-->
    <building>
      <soundAmbient>GeothermalPlant_Ambience</soundAmbient>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
    </building>
    <constructEffect>ConstructMetal</constructEffect>
    <!--<designationHotKey>Misc6</designationHotKey>-->
    <researchPrerequisites>
      <li>Research_RennReactor</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
  </ThingDef>
  <!-- RennPortal -->
  <ThingDef ParentName="BuildingBase"  Class="DanielRenner.RennOrganisms.Building_RennBaseDef">
    <defName>RennPortal</defName>
    <label>Renn gate</label>
    <description>A gateway between the realm of men and the native realm of the renn organisms. Renn are non-sentient beings living in multiple dimensions at the same time. Their this-worldly form are microbes that are invisibly small. \nThey have a peculiar effect to reality in our dimension: As long as they are well fed, they will try to maintain the source of their food, e.g. us. How they are doing this is not yet explainable, but it results in what is commonly known as luck: External threats to our society like raids are heavily reduced. Besides nutrition, they also feed on our colonists mood causing social infights in colonies not perfectly managed. One can't have everything, it seems.\n\nRenn gates as long as powered offer a flat reduction of threats by 20000 threat points or 36% - whatever is bigger.\nThey impose a -9 penalty to the mood of your people.</description>
    <thingClass>DanielRenner.RennOrganisms.Building_RennPortal</thingClass>
    <drawerType>MapMeshAndRealTime</drawerType>
    <graphicData>
      <texPath>Building_RennPortal/RennPortal</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(6,6)</drawSize>
      <!--<shadowData>
        <volume>(6.03,1.0,5.25)</volume>
        <offset>(0,0,-0.25)</offset>
      </shadowData>-->
      <damageData>
        <rect>(0.3,0,5.4,6)</rect>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <!--<interactionCellOffset>(1,0,-2)</interactionCellOffset>
    <hasInteractionCell>true</hasInteractionCell>-->
    <blockWind>true</blockWind>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <fillPercent>1</fillPercent>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>5000</MaxHitPoints>
      <WorkToBuild>12000</WorkToBuild>
      <Flammability>0.0</Flammability>
      <Beauty>10</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <size>(6,6)</size>
    <costList>
      <Steel>500</Steel>
      <ComponentIndustrial>12</ComponentIndustrial>
      <ComponentSpacer>4</ComponentSpacer>
    </costList>
    <baseMoodEffectOfBuildingUnpoweredAndUnfueled>0</baseMoodEffectOfBuildingUnpoweredAndUnfueled>
    <moodOffsetOfBuildingPowered>9</moodOffsetOfBuildingPowered>
    <moodToThreatMultiplier>0.04</moodToThreatMultiplier>
    <maxThreatCap>500000</maxThreatCap>
    <minThreatReduction>5000</minThreatReduction>
    <moodOffsetFedRennFiber>3</moodOffsetFedRennFiber>
    <moodOffsetFedRennFiberDomestic>6</moodOffsetFedRennFiberDomestic>
    <moodOffsetFedRennPowder>9</moodOffsetFedRennPowder>
    <moodOffsetFedRennConcentrate>12</moodOffsetFedRennConcentrate>
    <moodOffsetFedRennCapsule>15</moodOffsetFedRennCapsule>
    <availableFeedOptions>
      <li>RennCapsule</li>
    </availableFeedOptions>
    <comps>
      <li Class="CompProperties_Flickable"/>
      <li Class="DanielRenner.RennOrganisms.CompProperties_SpecificRefuelable">
        <fuelConsumptionRate>10.0</fuelConsumptionRate>
        <fuelCapacity>30.0</fuelCapacity>
        <consumeFuelOnlyWhenPowered>true</consumeFuelOnlyWhenPowered>
        <fuelFilter>
          <thingDefs>
            <li>RennCapsule</li>
          </thingDefs>
        </fuelFilter>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>4</glowRadius>
        <glowColor>(80,112,180,0)</glowColor>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>false</shortCircuitInRain>
        <basePowerConsumption>5000</basePowerConsumption>
      </li>
    </comps>
    <rotatable>false</rotatable>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
    <!--<placeWorkers>
      <li>PlaceWorker_OnSteamGeyser</li>
    </placeWorkers>-->
    <building>
      <!--<ai_chillDestination>false</ai_chillDestination>-->
      <soundAmbient>GeothermalPlant_Ambience</soundAmbient>
      <destroySound>BuildingDestroyed_Metal_Big</destroySound>
    </building>
    <constructEffect>ConstructMetal</constructEffect>
    <!--<designationHotKey>Misc6</designationHotKey>-->
    <researchPrerequisites>
      <li>Research_RennPortal</li>
    </researchPrerequisites>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
  </ThingDef>
  <!-- Plant_RennFiber -->
  <ThingDef ParentName="PlantBase">
    <defName>Plant_RennFiber</defName>
    <label>renn fiber plant</label>
    <description>A crop that yields renn fibers. A hardy fiber that is digested by the name-giving renn microbes.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0.2</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Plant_RennFiber/RennFiber</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>14</pathCost>
    <plant>
      <growDays>8</growDays>
      <dieIfLeafless>true</dieIfLeafless>
      <harvestTag>Standard</harvestTag>
      <immatureGraphicPath>Plant_RennFiber/RennFiber_Immature</immatureGraphicPath>
      <harvestedThingDef>RennFiber</harvestedThingDef>
      <harvestYield>1</harvestYield>
      <sowTags>
        <li>Ground</li>
      </sowTags>
      <sowResearchPrerequisites>
        <li>Research_RennFiber</li>
      </sowResearchPrerequisites>
      <topWindExposure>0.1</topWindExposure>
      <visualSizeRange>0.3~1.05</visualSizeRange>
